{
	"name": "@alexaegis/workspace",
	"displayName": "workspace",
	"description": "Vite plugin",
	"version": "0.0.14",
	"license": "mit",
	"private": true,
	"author": {
		"email": "alexaegis@gmail.com",
		"name": "Alex Aegis",
		"url": "https://github.com/AlexAegis"
	},
	"homepage": "https://github.com/AlexAegis/vite-plugin-autolib",
	"bugs": {
		"email": "alexaegis@gmail.com",
		"url": "https://github.com/AlexAegis/vite-plugin-autolib/issues"
	},
	"keywords": [
		"eslint",
		"javascript",
		"js",
		"ts",
		"tsconfig",
		"turbo",
		"typescript"
	],
	"type": "module",
	"config": {
		"engine-strict": true
	},
	"engines": {
		"node": ">=18.13.0"
	},
	"packageManager": "pnpm@8.1.1",
	"workspaces": [
		"apps/*",
		"libs/*",
		"packages/*"
	],
	"scripts": {
		"build": "turbo run build-lib_ build-app_",
		"build-lib": "turbo run build-lib_",
		"lint": "turbo run lint_",
		"lint:depcheck": "turbo run lint:depcheck_ --concurrency 6",
		"lint:depcheck_": "depcheck",
		"lint:es": "turbo run lint:es_ --concurrency 6",
		"lint:format": "turbo run lint:format_ --concurrency 6",
		"lint:format_": "prettier --check *.{json,ts,js,mjs,md,yml,yaml}",
		"lint:md": "turbo run lint:md_ --concurrency 6",
		"lint:md_": "remark --frail --no-stdout --silently-ignore *.md docs/**/*.md",
		"lint:md:workspace": "markdownlint --ignore-path '.gitignore' --ignore CHANGELOG.md *.md",
		"lint:rmd": "turbo run lint:rmd --parallel",
		"lint:style": "turbo run lint:style --parallel",
		"lint:svelte": "turbo run lint:svelte --parallel",
		"lint:tsc": "turbo run lint:tsc_ --concurrency 6",
		"ce": "turbo run ce_",
		"format": "prettier --write .",
		"merge-coverage": "merge-workspace-lcov-reports && lcov-viewer lcov -o ./coverage ./coverage/lcov.info",
		"ncu": "ncu --deep --peer --upgrade",
		"nuke": "nuke",
		"postinstall": "husky install",
		"precommit": "pnpm install",
		"release": "commit-and-tag-version --sign --commit-all",
		"release:major": "commit-and-tag-version --sign --commit-all --release-as major",
		"release:minor": "commit-and-tag-version --sign --commit-all --release-as minor",
		"release:patch": "commit-and-tag-version --sign --commit-all --release-as patch",
		"setup-all": "npx setup-prettier && npx setup-editorconfig && npx setup-husky && npx setup-git && npx setup-commitlint && npx setup-eslint && npx setup-github && npx setup-ts && npx setup-remark && npx setup-standard-version && npx setup-stylelint && npx setup-turbo && npx setup-vite && npx setup-vitest && npx setup-workspace",
		"start": "turbo run start --parallel",
		"test": "turbo run test_ --concurrency 6",
		"typedoc": "turbo run typedoc"
	},
	"devDependencies": {
		"@alexaegis/coverage-tools": "^0.1.1",
		"@alexaegis/eslint-config-core": "^0.1.0",
		"@alexaegis/nuke": "^0.1.0",
		"@alexaegis/setup-commitlint": "0.1.0",
		"@alexaegis/setup-depcheck": "0.1.0",
		"@alexaegis/setup-editorconfig": "0.1.0",
		"@alexaegis/setup-eslint": "0.1.0",
		"@alexaegis/setup-git": "0.1.0",
		"@alexaegis/setup-github": "0.1.0",
		"@alexaegis/setup-husky": "0.1.0",
		"@alexaegis/setup-prettier": "0.1.0",
		"@alexaegis/setup-remark": "0.1.0",
		"@alexaegis/setup-standard-version": "0.1.0",
		"@alexaegis/setup-stylelint": "0.1.0",
		"@alexaegis/setup-ts": "0.1.0",
		"@alexaegis/setup-turbo": "0.1.0",
		"@alexaegis/setup-vite": "0.1.0",
		"@alexaegis/setup-vitest": "0.1.0",
		"@alexaegis/setup-vscode": "0.1.0",
		"@alexaegis/setup-workspace": "0.1.0",
		"@alexaegis/ts": "0.1.0",
		"@lcov-viewer/cli": "^1.3.0",
		"@types/eslint": "^8.37.0",
		"@typescript-eslint/eslint-plugin": "^5.57.1",
		"@typescript-eslint/parser": "^5.57.1",
		"@vitest/coverage-c8": "^0.29.8",
		"commit-and-tag-version": "^11.2.1",
		"commitlint": "^17.5.1",
		"depcheck": "^1.4.3",
		"eslint": "^8.38.0",
		"eslint-config-next": "^13.3.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-config-turbo": "^1.8.8",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-unicorn": "^46.0.0",
		"husky": "^8.0.3",
		"lint-staged": "^13.2.1",
		"markdownlint-cli": "^0.33.0",
		"npm-check-updates": "^16.10.7",
		"prettier": "^2.8.7",
		"remark-cli": "^11.0.0",
		"stylelint": "15.4.0",
		"ts-node": "^10.9.1",
		"turbo": "^1.8.8",
		"typedoc": "^0.23.28",
		"typescript": "^5.0.4",
		"vitest": "^0.29.8"
	}
}
